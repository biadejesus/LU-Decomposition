platform_libraries = -L$(top_builddir) -lgasnet_tools-seq     $(blcr_ldflags)
amxdir = $(srcdir)/../amx
altincdir = $(srcdir)/..
gasnet_tools = -I$(srcdir)/../.. -I../.. -DHAVE_GASNET_TOOLS
INCLUDES = -I$(srcdir) -I. -I$(amxdir) -I$(altincdir) $(platform_defines) $(gasnet_tools)
