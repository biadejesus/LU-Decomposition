@UPCR_OBJECT_STRINGS_FALSE@UPCR_SIZES_HEADER = upcr_sizes-$(UPCR_CONDUIT)-$(GASNET_PARSEQ).h
@UPCR_OBJECT_STRINGS_FALSE@UPCR_SIZES_ACTIVATE = -DUPCRI_SIZES_H=$(UPCR_SIZES_HEADER)
